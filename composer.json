{
    "name": "tmhcc-cplg/notes",
    "description": "A simple notes web application",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-pdo_mysql": "*",
        "ext-posix": "*",
        "ext-simplexml": "*",
        "ext-tokenizer": "*",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.12",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/console": "^5.4.0",
        "symfony/dotenv": "^5.4.0",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.4.0",
        "symfony/property-access": "^5.4.0",
        "symfony/property-info": "^5.4.0",
        "symfony/proxy-manager-bridge": "^5.4.0",
        "symfony/runtime": "^5.4.0",
        "symfony/serializer": "^5.4.0",
        "symfony/twig-bundle": "^5.4.0",
        "symfony/validator": "^5.4.0",
        "symfony/yaml": "^5.4.0"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^5.4.0",
            "docker": false
        }
    },
    "require-dev": {
        "ext-xmlwriter": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpspec/phpspec": "*",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/maker-bundle": "*",
        "symfony/phpunit-bridge": "^6.1"
    }
}
